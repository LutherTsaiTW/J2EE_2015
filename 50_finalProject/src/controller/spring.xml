<?xml version="1.0" encoding="UTF-8"?> 
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<bean id="REGISTER" class="java.lang.String">
		<constructor-arg type="String" value="finalProject/RegisterPage"/>
    </bean>
    
    <bean id="LOGIN" class="java.lang.String">
		<constructor-arg type="String" value="finalProject/LoginPage"/>
    </bean>
    
    <bean id="ERROR" class="java.lang.String">
		<constructor-arg type="String" value="finalProject/error"/>
    </bean>
    
    <bean id="member_system" class="java.lang.String">
		<constructor-arg type="String" value="http://localhost:8080/50_finalProject/spring/webservice/member" />
	</bean>
</beans>