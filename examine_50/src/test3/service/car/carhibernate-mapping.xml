<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
  	<hibernate-mapping>
  		<!-- Set Model and Table -->
  		<class name="test3.model.CarModel" table="Car">
    		<id name="id" column="id" type="integer">
      			<generator class="identity"/>
  		  	</id>
    		<property name="name" column="name" type="string" not-null="true" length="50"/>
    		<property name="price" column="price" type="integer" not-null="true" length="50"/>
    		<property name="ownerId" column="ownerId" type="integer" not-null="true" length="50"/>
  		</class>
	</hibernate-mapping>